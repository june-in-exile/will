{
  "name": "zkp",
  "version": "1.0.0",
  "description": "TypeScript testing framework for Circom circuit development",
  "type": "module",
  "scripts": {
    "test": "vitest run",
    "test:watch": "vitest",
    "test:abiEncoder": "pnpm exec tsx ./tests/logic/modules/abiEncoder.ts",
    "test:keccak256": "pnpm exec tsx ./tests/logic/modules/keccak256.ts",
    "test:aesgcm": "pnpm exec tsx ./tests/logic/modules/aesGcm.ts",
    "test:ecdsa": "pnpm exec tsx ./tests/logic/modules/ecdsa.ts",
    "test:permitVerify": " pnpm exec tsx ./tests/logic/modules/permitVerify.ts",
    "test:all": "pnpm exec tsx ./tests/logic/modules/runTests.ts && vitest run --reporter=basic",
    "e2e": "make circuit",
    "lint": "eslint '**/*.ts'",
    "format": "prettier --write '**/*.ts'",
    "clean": "make clean-all"
  },
  "keywords": [
    "zkp",
    "zero-knowledge proof",
    "circom",
    "zk-snarks",
    "aes",
    "aes-ctr",
    "aesGcm"
  ],
  "author": "June",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^8.35.1",
    "@typescript-eslint/parser": "^8.35.1",
    "circom_tester": "0.0.24",
    "eslint": "^8.57.1",
    "prettier": "^3.0.0",
    "ts-node": "^10.9.0",
    "typescript": "^5.8.3",
    "vitest": "^2.1.8"
  },
  "dependencies": {
    "circomlib": "^2.0.5",
    "keccak256-circom": "git+https://github.com/vocdoni/keccak256-circom",
    "snarkjs": "^0.7.5"
  }
}
