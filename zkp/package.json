{
  "name": "zkp",
  "version": "1.0.0",
  "description": "TypeScript testing framework for Circom circuit development",
  "type": "module",
  "scripts": {
    "test": "vitest run",
    "test:watch": "vitest",
    "test:all": "pnpm exec tsx ./tests/logic/aes-gcm.ts && pnpm exec tsx ./tests/logic/keccak256.ts && vitest run --reporter=basic",
    "e2e": "make circuit",
    "lint": "eslint '**/*.ts'",
    "format": "prettier --write '**/*.ts'",
    "clean": "make clean-all"
  },
  "keywords": [
    "zkp",
    "zero-knowledge proof",
    "circom",
    "zk-snarks",
    "aes",
    "aes-ctr",
    "aes-gcm"
  ],
  "author": "June",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^8.35.1",
    "@typescript-eslint/parser": "^8.35.1",
    "circom_tester": "0.0.24",
    "eslint": "^8.57.1",
    "prettier": "^3.0.0",
    "ts-node": "^10.9.0",
    "typescript": "^5.8.3",
    "vitest": "^2.1.8"
  },
  "dependencies": {
    "circomkit": "^0.3.3",
    "circomlib": "^2.0.5",
    "snarkjs": "^0.7.5"
  }
}